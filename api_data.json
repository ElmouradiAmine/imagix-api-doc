        {
          "title": "Error-Response-InvalidCredentiels:",
          "content": "HTTP/1.1 400 Bad Request\n{\n  \"error\": \"InvalidCredentiels\"\n}",
          "type": "json"
        }
      ]
    },
    "filename": "./api/controllers/user.js",
    "groupTitle": "User"
  }
